	<queryString>
		<![CDATA[select g.descricao
     , it.nome
     , it.unidade
     , gi.quantidade
     , g.id
     , g.valor1
     , g.valor2
     , g.valor3
  from grupo g
  join grupo_item gi
  on gi.grupo_id = g.id
  join item it
  on it.id = gi.item_id
 where g.id  = $P{cod_grupo}]]>
	</queryString>
