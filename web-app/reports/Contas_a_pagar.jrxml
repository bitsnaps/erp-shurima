	</parameter>
	<parameter name="data_final" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select e.nome
     , t.data
     , t.valor
     , t.numero_titulo
     , t.observacao
  from transacao t
  join empresa e
    on e.id = t.empresa_id
 where date_trunc('day',t.data) between $P{data_inicial} and $P{data_final}
   and t.tipo = 'P'
 order by t.data]]>
	</queryString>
